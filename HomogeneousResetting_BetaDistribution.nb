1a9aa12f336e"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Tau]0", "[", 
    RowBox[{"r_", ",", "x_"}], "]"}], ":=", 
   RowBox[{
    SuperscriptBox["r", 
     RowBox[{"-", "1"}]], "*", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Cosh", "[", 
       RowBox[{
        RowBox[{"Sqrt", "[", "r", "]"}], "x"}], "]"}], "-", "1", "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Cosh", "[", 
          RowBox[{"Sqrt", "[", "r", "]"}], "]"}], "-", "1"}], ")"}], 
       FractionBox[
        RowBox[{"Sinh", "[", 
         RowBox[{
          RowBox[{"Sqrt", "[", "r", "]"}], 
          RowBox[{"Abs", "[", "x", "]"}]}], "]"}], 
        RowBox[{"Sinh", "[", 
         RowBox[{"Sqrt", "[", "r", "]"}], "]"}]]}]}], ")"}]}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.956373381311529*^9, 3.956373398107012*^9}},
 CellLabel->"In[7]:=",ExpressionUUID->"b44fd778-2bca-43c3-81e6-863d4941bbec"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Series", "[", 
  RowBox[{
   RowBox[{"\[Tau]0", "[", 
    RowBox[{"r", ",", "xT"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"r", ",", "0", ",", "1"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.9563734047089767`*^9, 3.956373407728075*^9}},
