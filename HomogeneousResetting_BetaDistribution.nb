Cell[BoxData[
 InterpretationBox[
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["xT", "2"], "+", 
      RowBox[{"Abs", "[", "xT", "]"}]}], ")"}]}], "+", 
   RowBox[{
    FractionBox["1", "24"], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["xT", "4"], "-", 
      RowBox[{"Abs", "[", "xT", "]"}], "+", 
      RowBox[{"2", " ", 
       SuperscriptBox[
        RowBox[{"Abs", "[", "xT", "]"}], "3"]}]}], ")"}], " ", "r"}], "+", 
   InterpretationBox[
    SuperscriptBox[
     RowBox[{"O", "[", "r", "]"}], 
     RowBox[{"3", "/", "2"}]],
    SeriesData[$CellContext`r, 0, {}, 0, 3, 2],
    Editable->False]}],
  SeriesData[$CellContext`r, 0, {
   Rational[1, 2] ($CellContext`xT^2 + Abs[$CellContext`xT]), 0, 
    Rational[1, 24] ($CellContext`xT^4 - Abs[$CellContext`xT] + 
     2 Abs[$CellContext`xT]^3)}, 0, 3, 2],
  Editable->False]], "Output",
 CellChangeTimes->{3.9472186324439373`*^9, 3.956373408607216*^9},
 CellLabel->"Out[8]=",ExpressionUUID->"a55ebf92-0ef8-4170-8b5e-ace1163c5d1a"]
}, Open  ]]
